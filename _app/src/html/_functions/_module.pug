  function sha256(text) {
    var hash = 0, i, chr;
    if (text.length === 0) return hash;
    for (i = 0; i < text.length; i++) {
        chr = text.charCodeAt(i);
        hash = ((hash << 5) - hash) + chr;
        hash |= 0;
    }
    return Math.abs(hash);
  }

  function module(name, data = {}) {
    const hash = sha256(JSON.stringify(data, null, 2));
    const moduleName = `${name}_m_${hash}`;
    moduleOptions = Object.assign(moduleOptions, {[moduleName]: data});
    return moduleName;
  }
